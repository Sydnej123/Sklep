                              <TextField fx:id="loginField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                              <PasswordField fx:id="passwordField" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                           </children>
                        </GridPane>
                     </content>
                  </Tab>
                  <Tab closable="false" text="Rejestracja">
